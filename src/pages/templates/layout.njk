{% set m = datasets.get('meta') %}

<!doctype html>
<html class="no-js" lang="{{ m.lang }}">

<head>
  <meta charset="{{ m.charset }}">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <title>{{ m.title }}</title>
  <meta name="description" content="{{ m.description }}">
  <meta name="viewport" content="width=device-width">

  {% if m.og.use == true %}
  <meta property="og:title" content="{{ m.og.title }}" />
  <meta property="og:url" content="{{ m.og.url }}" />
  <meta property="og:site_name" content="{{ m.og.site_name }}" />
  <meta property="og:type" content="website" />
  <meta property="og:image" content="{{ m.og.url + '/assets/imgs/breachreport-og-logotype@1200x630.png' }}" />
  <meta property="og:image:width" content="1200" />
  <meta property="og:image:height" content="630" />
  <meta property="og:description" content="{{ m.og.descr }}" />
  {% endif %}

  {% if m.favicon %}
  <link rel="shortcut icon" href="favicon.ico">
  {% endif %}

  <link rel="stylesheet" href="css/main.css">
</head>

<body data-spy="scroll" data-target="#navbarNavDropdown" class="position-relative">
  <!--[if lte IE 9]>
  <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
  <![endif]-->

  {% include "partials/preloader.njk" %}

  {% include "partials/header.njk" %}

  {% block content %} {% endblock %}


  <script src="js/vendor.js" charset="utf-8"></script>
  <script src="js/jquery.3.3.1.min.js" charset="utf-8"></script>
  <script src="js/popper.min.js" charset="utf-8"></script>
  <script src="js/bootstrap.min.js" charset="utf-8"></script>
  <script src="js/jquery.appear.js" charset="utf-8"></script>
  <script src="js/modernizr.js" charset="utf-8"></script>
  <script src="js/jquery.easing.min.js" charset="utf-8"></script>
  <script src="js/menu/jquery.smartmenus.js" charset="utf-8"></script>
  <script src="js/owl-carousel/owl.carousel.min.js" charset="utf-8"></script>
  <script src="js/magnific-popup/jquery.magnific-popup.min.js" charset="utf-8"></script>
  <script src="js/counter/counter.js" charset="utf-8"></script>
  <script src="js/countdown/jquery.countdown.min.js" charset="utf-8"></script>
  <script src="js/contact-form/contact-form.js" charset="utf-8"></script>
  <script src="js/contact-form/jquery.validate.min.js" charset="utf-8"></script>
  <script src="https://maps.googleapis.com/maps/api/js"></script>
  <script src="js/map.js" charset="utf-8"></script>
  <script src="js/wow.min.js" charset="utf-8"></script>
  <script src="js/theme-script.js" charset="utf-8"></script>
  <script src="js/main.js" charset="utf-8"></script>

</body>

</html>
